<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Library</display-name>
  <welcome-file-list>
    <welcome-file>landing.html</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>com.library.login.LibraryLogin</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>buybook</servlet-name>
  <servlet-class>com.library.buybooks.LibraryBuyBook</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>buybook</servlet-name>
  <url-pattern>/buybook</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>signup</servlet-name>
  <servlet-class>com.signup.UserSignUp</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>sigup</servlet-name>
  <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
</web-app>